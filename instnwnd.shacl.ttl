@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix nw: <http://example.org/northwind#> .

# SHACL Shapes for Northwind Database

# Employees
nw:EmployeeShape
  a sh:NodeShape ;
  sh:targetClass nw:Employees ;
  sh:property [
    sh:path nw:EmployeeID ;
    sh:datatype xsd:integer ;
    sh:name "EmployeeID" ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
  ] ;
  sh:property [
    sh:path nw:LastName ;
    sh:datatype xsd:string ;
    sh:name "LastName" ;
    sh:minLength 1 ;
    sh:maxLength 20 ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
  ] ;
  sh:property [
    sh:path nw:FirstName ;
    sh:datatype xsd:string ;
    sh:name "FirstName" ;
    sh:minLength 1 ;
    sh:maxLength 10 ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
  ] ;
  sh:property [
    sh:path nw:Title ;
    sh:datatype xsd:string ;
    sh:name "Title" ;
    sh:maxLength 30 ;
    sh:maxCount 1 ;
  ] ;
  sh:property [
    sh:path nw:TitleOfCourtesy ;
    sh:datatype xsd:string ;
    sh:name "TitleOfCourtesy" ;
    sh:maxLength 25 ;
    sh:maxCount 1 ;
  ] ;
  sh:property [
    sh:path nw:BirthDate ;
    sh:datatype xsd:dateTime ;
    sh:name "BirthDate" ;
    sh:maxCount 1 ;
  ] ;
  sh:property [
    sh:path nw:HireDate ;
    sh:datatype xsd:dateTime ;
    sh:name "HireDate" ;
    sh:maxCount 1 ;
  ] ;
  sh:property [
    sh:path nw:Address ;
    sh:datatype xsd:string ;
    sh:name "Address" ;
    sh:maxLength 60 ;
    sh:maxCount 1 ;
  ] ;
  sh:property [
    sh:path nw:City ;
    sh:datatype xsd:string ;
    sh:name "City" ;
    sh:maxLength 15 ;
    sh:maxCount 1 ;
  ] ;
  sh:property [
    sh:path nw:Region ;
    sh:datatype xsd:string ;
    sh:name "Region" ;
    sh:maxLength 15 ;
    sh:maxCount 1 ;
  ] ;
  sh:property [
    sh:path nw:PostalCode ;
    sh:datatype xsd:string ;
    sh:name "PostalCode" ;
    sh:maxLength 10 ;
    sh:maxCount 1 ;
  ] ;
  sh:property [
    sh:path nw:Country ;
    sh:datatype xsd:string ;
    sh:name "Country" ;
    sh:maxLength 15 ;
    sh:maxCount 1 ;
  ] ;
  sh:property [
    sh:path nw:HomePhone ;
    sh:datatype xsd:string ;
    sh:name "HomePhone" ;
    sh:maxLength 24 ;
    sh:maxCount 1 ;
  ] ;
  sh:property [
    sh:path nw:Extension ;
    sh:datatype xsd:string ;
    sh:name "Extension" ;
    sh:maxLength 4 ;
    sh:maxCount 1 ;
  ] ;
  sh:property [
    sh:path nw:Notes ;
    sh:datatype xsd:string ;
    sh:name "Notes" ;
    sh:maxCount 1 ;
  ] ;
  sh:property [
    sh:path nw:ReportsTo ;
    sh:name "ReportsTo" ;
    sh:class nw:Employees ;
    sh:maxCount 1 ;
  ] ;
  sh:property [
    sh:path nw:PhotoPath ;
    sh:datatype xsd:string ;
    sh:name "PhotoPath" ;
    sh:maxLength 255 ;
    sh:maxCount 1 ;
  ] .


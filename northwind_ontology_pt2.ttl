<http://example.org/northwind> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

nw:ReportsTo rdf:type owl:ObjectProperty ;
  rdfs:domain nw:Employees ;
  rdfs:range nw:Employees .

nw:CustomerID rdf:type owl:ObjectProperty ;
  rdfs:domain nw:Orders, nw:CustomerCustomerDemo ;
  rdfs:range nw:Customers .

nw:EmployeeID rdf:type owl:ObjectProperty ;
  rdfs:domain nw:Orders, nw:EmployeeTerritories ;
  rdfs:range nw:Employees .

nw:ShipVia rdf:type owl:ObjectProperty ;
  rdfs:domain nw:Orders ;
  rdfs:range nw:Shippers .

nw:SupplierID rdf:type owl:ObjectProperty ;
  rdfs:domain nw:Products ;
  rdfs:range nw:Suppliers .

nw:CategoryID rdf:type owl:ObjectProperty ;
  rdfs:domain nw:Products ;
  rdfs:range nw:Categories .

nw:OrderID rdf:type owl:ObjectProperty ;
  rdfs:domain nw:Order_Details ;
  rdfs:range nw:Orders .

nw:ProductID rdf:type owl:ObjectProperty ;
  rdfs:domain nw:Order_Details ;
  rdfs:range nw:Products .

nw:RegionID rdf:type owl:ObjectProperty ;
  rdfs:domain nw:Territories ;
  rdfs:range nw:Region .

nw:TerritoryID rdf:type owl:ObjectProperty ;
  rdfs:domain nw:EmployeeTerritories ;
  rdfs:range nw:Territories .

nw:CustomerTypeID rdf:type owl:ObjectProperty ;
    rdfs:domain nw:CustomerCustomerDemo ;
    rdfs:range nw:CustomerDemographics .


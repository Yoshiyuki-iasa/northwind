#################################################################
#    Entity Concepts (Architypes)
#################################################################

@prefix arc: <http://example.org/architypes#> .

arc:architype rdf:type owl:AnnotationProperty ;
    rdfs:label "Architype root" .

arc:Resource rdf:type owl:AnnotationProperty ;
    rdfs:subPropertyOf arc:architype ;
    rdfs:comment "主語または目的語となる名詞の集合。顧客、従業員、商品、勘定科目など。" ;
    rdfs:label "Resource" .

arc:Event rdf:type owl:AnnotationProperty ;
    rdfs:subPropertyOf arc:architype ;
    rdfs:comment "時間と共に発生する状態変化の記録。受注や出荷。" ;
    rdfs:label "Event" .

arc:Classification rdf:type owl:AnnotationProperty ;
    rdfs:subPropertyOf arc:architype ;
    rdfs:comment "MECEな概念の集合。国、都道府県、ステータスなど。" ;
    rdfs:label "Classification" .

arc:Rule rdf:type owl:AnnotationProperty ;
    rdfs:subPropertyOf arc:architype ;
    rdfs:comment "振る舞いを規定するための複数の概念のあらかじめ定められた組み合わせ。値引きルール、返品ルールなど。" ;
    rdfs:label "Rule" .

arc:Summary rdf:type owl:AnnotationProperty ;
    rdfs:subPropertyOf arc:architype ;
    rdfs:comment "別のデータセットから導出された集計結果。総勘定元帳など。" ;
    rdfs:label "Summary" .

arc:Association rdf:type owl:AnnotationProperty ;
    rdfs:subPropertyOf arc:architype ;
    rdfs:comment "複数のエンティティ間の関係についての事実。営業テリトリーと営業員など。" ;
    rdfs:label "Association" .

#################################################################
#    Attribute Concepts (Semantic Types)
#################################################################

@prefix ac: <http://example.org/attribute-concepts#> .

ac:semanticType rdf:type owl:AnnotationProperty ;
    rdfs:label "Semantic Type root" .

ac:ID rdf:type owl:AnnotationProperty ;
    rdfs:subPropertyOf ac:semanticType ;
    rdfs:comment "リソースのキー" ;
    rdfs:label "Identifier" .

ac:Sequence rdf:type owl:AnnotationProperty ;
    rdfs:subPropertyOf ac:semanticType ;
    rdfs:comment "イベントのキー" ;
    rdfs:label "Sequence" .

ac:Classifier rdf:type owl:AnnotationProperty ;
    rdfs:subPropertyOf ac:semanticType ;
    rdfs:comment "分類のキー" ;
    rdfs:label "Classifier" .

ac:Enum rdf:type owl:AnnotationProperty ;
    rdfs:subPropertyOf ac:semanticType ;
    rdfs:comment "分類テーブルがない (リテラル) 区分値" ;
    rdfs:label "Enum" .

ac:Flag rdf:type owl:AnnotationProperty ;
    rdfs:subPropertyOf ac:semanticType ;
    rdfs:comment "区分値または列挙子のうち、特に真偽二値のみのもの" ;
    rdfs:label "Flag" .

ac:Name rdf:type owl:AnnotationProperty ;
    rdfs:subPropertyOf ac:semanticType ;
    rdfs:comment "識別子または区分値の名称" ;
    rdfs:label "Name" .

ac:NumValue rdf:type owl:AnnotationProperty ;
    rdfs:subPropertyOf ac:semanticType ;
    rdfs:comment "単位を伴い計算可能な、時刻・時間以外の数値" ;
    rdfs:label "NumValue" .

ac:TimeStamp rdf:type owl:AnnotationProperty ;
    rdfs:subPropertyOf ac:semanticType ;
    rdfs:comment "時間軸上の一点を示す値" ;
    rdfs:label "TimeStamp" .

ac:Duration rdf:type owl:AnnotationProperty ;
    rdfs:subPropertyOf ac:semanticType ;
    rdfs:comment "時間軸上の二点間の間隔を示す値" ;
    rdfs:label "Duration" .

ac:ShortText rdf:type owl:AnnotationProperty ;
    rdfs:subPropertyOf ac:semanticType ;
    rdfs:comment "汎用的な短い文字列" ;
    rdfs:label "ShortText" .

ac:LongText rdf:type owl:AnnotationProperty ;
    rdfs:subPropertyOf ac:semanticType ;
    rdfs:comment "自由記述の文章" ;
    rdfs:label "LongText" .

ac:FormattedText rdf:type owl:AnnotationProperty ;
    rdfs:subPropertyOf ac:semanticType ;
    rdfs:comment "特定の様式を伴う文字列" ;
    rdfs:label "FormattedText" .

ac:AddressComponent rdf:type owl:AnnotationProperty ;
    rdfs:subPropertyOf ac:semanticType ;
    rdfs:comment "住所を構成する文字要素" ;
    rdfs:label "AddressComponent" .

ac:Binary rdf:type owl:AnnotationProperty ;
    rdfs:subPropertyOf ac:semanticType ;
    rdfs:comment "画像・音声などのバイナリデータ" ;
    rdfs:label "Binary" .

